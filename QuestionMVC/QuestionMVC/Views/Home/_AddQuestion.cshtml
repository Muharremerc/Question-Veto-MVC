@model DAL.Model.Content
@{
    if (Model == null)
    {
        Response.Redirect("~/Login/Index");
    }
}
<h2><i class="fa fa-angle-right"></i> @Model.Name </h2>
<div class="form-panel">
    <div class=" form">
        <form class="cmxform form-horizontal style-form" id="commentForm">
            <div class="form-group ">
                <label for="cname" class="control-label col-lg-1">Question Details</label>
                <div class="col-lg-10">
                    <input class=" form-control" id="qDetails" name="qDetails" minlength="2" type="text" required="">
                </div>
            </div>
            <div class="form-group ">
                <label for="cname" class="control-label col-lg-1">Score</label>
                <div class="col-lg-10">
                    <input class=" form-control" id="score" name="score" minlength="2" type="text" required="">
                </div>
            </div>
            <div class="form-group">

                <div class="col-sm-10">
                    <a href="javascript:addQuestion()" class="btn btn-theme">Add Question</a>
                    <a href="javascript:FinishContent()" class="btn btn-theme">Finish</a>
                </div>
            </div>
           
        </form>
    </div>
</div>
